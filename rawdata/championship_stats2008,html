  <th data-stat="" align="CENTER" colspan=2  class="tooltip over_header" ></th>
  <th data-stat="header_pct_fga" align="center" colspan=6  class="bold_text over_header" >% of FGA by Distance</th>
  <th data-stat="header_fg_pct" align="center" colspan=6  class="bold_text over_header" >FG% by Distance</th>
  <th></th>
  <th data-stat="header_dunks" align="center" colspan=2  class="bold_text over_header" >Dunks</th>

<tr class="">
  <th data-stat="ranker" align="right"  class="ranker sort_default_asc show_partial_when_sorting"  tip="Rank">Rk</th>
  <th data-stat="team_name" align="left"  class="tooltip sort_default_asc" >Team</th>
  <th data-stat="g" align="right"  class="tooltip"  tip="Games">G</th>
  <th data-stat="mp" align="right"  class="tooltip"  tip="Minutes Played">MP</th>
  <th data-stat="fg_pct" align="right"  class="tooltip"  tip="Field Goal Percentage">FG%</th>
  <th data-stat="avg_dist" align="right"  class="tooltip"  tip="Average distance (ft.) of FGA">Dist.</th>
  <th data-stat="fg2a_pct_fga" align="right"  class="tooltip"  tip="% of FGAs that are 2-Pt FGAs.">2P</th>
  <th data-stat="pct_fga_00_03" align="right"  class="tooltip"  tip="% of FGAs that are 0-3 feet from the basket.">0-3</th>
  <th data-stat="pct_fga_03_10" align="right"  class="tooltip"  tip="% of FGAs that are 3-10 feet from the basket.">3-10</th>
  <th data-stat="pct_fga_10_16" align="right"  class="tooltip"  tip="% of FGAs that are 10-16 feet from the basket.">10-16</th>
  <th data-stat="pct_fga_16_XX" align="right"  class="tooltip"  tip="% of FGAs that are 2-Pt shots<br>and 16+ feet from the basket.">16 <3</th>
  <th data-stat="fg3a_pct_fga" align="right"  class="tooltip"  tip="% of FGAs that are 3-Pt FGAs.">3P</th>
  <th data-stat="fg2_pct" align="right"  class="tooltip"  tip="FG% on 2-Pt FGAs.">2P</th>
  <th data-stat="fg_pct_00_03" align="right"  class="tooltip"  tip="FG% on FGAs that are 0-3 feet from the basket.">0-3</th>
  <th data-stat="fg_pct_03_10" align="right"  class="tooltip"  tip="FG% on FGAs that are 3-10 feet from the basket.">3-10</th>
  <th data-stat="fg_pct_10_16" align="right"  class="tooltip"  tip="FG% on FGAs that are 10-16 feet from the basket.">10-16</th>
  <th data-stat="fg_pct_16_XX" align="right"  class="tooltip"  tip="FG% on 2-Pt FGAs that are 16+ feet from the basket.">16 <3</th>
  <th data-stat="fg3_pct" align="right"  class="tooltip"  tip="FG% on 3-Pt FGAs.">3P</th>
  <th data-stat="fg2_pct_ast" align="right"  class="tooltip"  tip="% of 2-Pt FGs that are assisted.">%Ast'd</th>
  <th data-stat="pct_fg2_dunk" align="right"  class="tooltip"  tip="% of all FGAs that are dunk attempts.">%FGA</th>
  <th data-stat="fg2_dunk" align="right"  class="tooltip"  tip="Number of successful throwdowns.<br>Some discrepancies for 2000-01 totals. Reliably reported as of the 2001-02 season.">Md.</th>
  <th data-stat="pct_fg2_layup" align="right"  class="tooltip"  tip="% of all team FGAs that are layup attempts.">%FGA</th>
  <th data-stat="fg2_layup" align="right"  class="tooltip"  tip="# of layups made.">Md.</th>
  <th data-stat="fg3_pct_ast" align="right"  class="tooltip"  tip="% of 3-Pt FGs that are assisted.">%Ast'd</th>
  <th data-stat="pct_fg3a_corner" align="right"  class="tooltip"  tip="% of 3PAs from the corner.">%3PA</th>
  <th data-stat="fg3_pct_corner" align="right"  class="tooltip"  tip="3P% on 3PAs from the corner.">3P%</th>
